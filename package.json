{
  "$schema": "https://json.schemastore.org/package",
  "name": "logos",
  "description": "A multi-purpose community bot built to cater to language-learning communities on Discord.",
  "license": "Apache-2.0",
  "version": "3.42.1",
  "type": "module",
  "keywords": [
    "discord",
    "bot",
    "definitions",
    "translation",
    "language-recognition",
    "games",
    "guides",
    "language",
    "linguistics",
    "dictionaries",
    "translators",
    "detectors",
    "localised"
  ],
  "homepage": "https://logos.wordcollector.co.uk",
  "repository": {
    "type": "git",
    "url": "https://github.com/vxern/logos"
  },
  "bugs": {
    "url": "https://github.com/vxern/logos",
    "email": "vxern@wordcollector.co.uk"
  },
  "contributors": [
    {
      "name": "Dorian \"vxern\" OszczÄ™da",
      "email": "vxern@wordcollector.co.uk",
      "url": "https://github.com/vxern"
    }
  ],
  "scripts": {
    "postinstall": "npm audit fix && tsx ./scripts/load-sentence-pairs.ts",
    "lint": "biome check ./src/ --apply-unsafe --organize-imports-enabled true && biome check ./test/ --apply-unsafe --organize-imports-enabled true",
    "test": "npx mocha --require ./test/setup.ts",
    "serve": "npx tsx ./src/index.ts",
    "start": "pm2 start ecosystem.config.cjs --instances 1 --exp-backoff-restart-delay=500"
  },
  "dependencies": {
    "@discordeno/bot": "^19.0.0-next",
    "dexonline-scraper": "^0.1.1",
    "dotenv": "^16.4.1",
    "event-stream": "^4.0.1",
    "fastest-levenshtein": "^1.0.16",
    "ioredis": "^5.3.2",
    "lavaclient": "^4.1.1",
    "loglevel": "^1.9.1",
    "nanoid": "^5.0.5",
    "object-hash": "^3.0.0",
    "parse-wiktionary": "^1.0.0",
    "ravendb": "^5.4.2",
    "tinyld": "^1.3.4",
    "youtube-sr": "^4.3.10"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@discordeno/types": "^19.0.0-next",
    "@types/chai": "^4.3.12",
    "@types/event-stream": "^4.0.5",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.16",
    "@types/object-hash": "^3.0.6",
    "chai": "^5.1.0",
    "mocha": "^10.3.0",
    "pm2": "^5.3.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
